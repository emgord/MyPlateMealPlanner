<fieldset>

    <%= form_for(:session_data, url: sessions_path) do |f| %>
    <legend><h1>Login</h1></legend>
        <%= f.label :email %>:
        <%= f.email_field :email, :required => true %>

        <%= f.label :password %>:
        <%= f.password_field :password, :required => true %><br>

        <%= f.submit "Login" %>
      <% end %>
</fieldset>
